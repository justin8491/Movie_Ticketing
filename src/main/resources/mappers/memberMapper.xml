<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.box.small.memberMapper">
    <select id="listAll" resultType="com.box.small.dto.Member">
        select * from member
    </select>

    <select id="cnt">
        select count(*) from member
    </select>
    <select id="selectOne" parameterType="String" resultType="com.box.small.dto.Member">
        select * from member
        where mem_name = #{mem_name}
    </select>
</mapper>