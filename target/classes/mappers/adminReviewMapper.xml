<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.box.small.adminReviewMapper">

	<!-- 리뷰 삭제 -->
	<update id="deleteReview">
		update review
		set rev_status = 0
		where rev_no = #{rev_no}
	</update>

</mapper>